     C                     ELSE
     C                     MOVE XPCDE1    XPCDE1
     C                     ENDIF
      *
     C           YDAT      IFEQ TDAT
     C                     MOVE *ZEROS    XDAT    80P
     C                     ELSE
     C                     MOVE YDAT      XDAT
     C                     ENDIF
      *
     C           YDAT1     IFEQ TDAT
     C                     MOVE XDAT      XDAT1   80
     C                     ELSE
     C                     MOVE YDAT1     XDAT1
     C                     ENDIF
      *
     C                     MOVE XSCDE     XSCDE
      *
     C           XSCDE1    IFEQ *BLANKS
     C                     MOVE XSCDE     XSCDE1  6
     C                     ELSE
     C                     MOVE XSCDE1    XSCDE1
      *
     C                     ENDIF
      *
     C                     MOVELXDAT1     ADAT1
     C                     MOVELXDAT      ADAT
